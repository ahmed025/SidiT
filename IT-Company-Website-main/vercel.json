{
  "rewrites": [
    { 
      "source": "/admin", 
      "destination": "/admin/index.html" 
    },
    { 
      "source": "/admin/(.*)", 
      "destination": "/admin/index.html" 
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
